{$R *.RES}
var
  hwnd : THandle;

begin

  hwnd := FindWindow(nil,Pchar('Coleta Cartão Ponto'));

  if hwnd = 0 then
    begin
      Application.Initialize;
      Application.Title := 'Coleta de Cartão Ponto';
      Application.CreateForm(TDMControle, DMControle);
      Application.CreateForm(TFRM_LOGIN, FRM_LOGIN);
      Application.Run;
    end
  else
    begin
      ShowWindow(hwnd,SW_NORMAL);
      SetForegroundWindow(hwnd);
    end;
end.

